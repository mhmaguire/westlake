=semantic_form_for @career_contact, url: "careers/#{career.id}/contact.js", html: {multipart: true, target: 'upload_frame'}, remote: true do |f|
	=#f.semantic_errors 
	=f.inputs do 
		=f.input :email
		=f.input :name 
		=f.input :resume, as: :file
	=f.actions do 
		=f.action :submit, label: "Submit Application"

%iframe#upload_frame(name="upload_frame" style="width:1px;height:1px;border:0px" src="about:blank")

